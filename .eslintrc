{
  "parser": "babel-eslint",
  "extends": "react",
  "env": {
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "rules": {
    "indent": "off",
    "space-before-function-paren": "off",
    "no-alert": "off",
    "no-unused-vars": "warn"
  }
}
